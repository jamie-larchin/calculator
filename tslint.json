{
    "defaultSeverity": "warning",
    "extends": ["tslint:recommended", "tslint-config-prettier"],
    "linterOptions": {
        "exclude": ["config/**/*.js", "node_modules/**/*.ts"]
    },
    "rules": {
        "max-line-length": [true, 80],
        "no-eval": false,
        "semicolon": [true, "always"]
    }
}
